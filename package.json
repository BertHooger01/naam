{
    "name": "dutch-name-generator",
    "version": "2.0.0",
    "description": "Generate authentic Dutch names using phonetic patterns, regional variations, and historical context",
    "main": "dist/lib/index.js",
    "types": "dist/lib/index.d.ts",
    "bin": {
        "dutch-names": "dist/cli/index.js"
    },
    "scripts": {
        "build": "tsc",
        "start": "node dist/cli/index.js",
        "dev": "ts-node src/cli/index.ts",
        "demo": "ts-node src/cli/index.ts demo",
        "cli": "ts-node src/cli/index.ts",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "lint": "eslint src/**/*.ts",
        "lint:fix": "eslint src/**/*.ts --fix",
        "clean": "rimraf dist",
        "prebuild": "npm run clean",
        "docs": "typedoc src/lib/index.ts --out docs/api"
    },
    "keywords": [
        "dutch",
        "names",
        "generator",
        "random",
        "typescript",
        "netherlands",
        "phonetic",
        "regional",
        "historical",
        "cli"
    ],
    "author": "Your Name",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/your-username/naam.git"
    },
    "bugs": {
        "url": "https://github.com/your-username/naamr/issues"
    },
    "homepage": "https://github.com/your-username/naam#readme",
    "dependencies": {
        "boxen": "^7.1.1",
        "chalk": "^5.3.0",
        "cli-table3": "^0.6.3",
        "commander": "^11.0.0",
        "csv-writer": "^1.6.0",
        "figlet": "^1.7.0",
        "gradient-string": "^2.0.2",
        "ora": "^7.0.1"
    },
    "devDependencies": {
        "@types/figlet": "^1.5.8",
        "@types/gradient-string": "^1.1.6",
        "@types/jest": "^29.5.0",
        "@types/node": "^20.0.0",
        "@typescript-eslint/eslint-plugin": "^5.59.0",
        "@typescript-eslint/parser": "^5.59.0",
        "eslint": "^8.40.0",
        "jest": "^29.5.0",
        "rimraf": "^5.0.0",
        "ts-jest": "^29.1.0",
        "ts-node": "^10.9.0",
        "typedoc": "^0.24.0",
        "typescript": "^5.0.0"
    },
    "files": [
        "dist",
        "README.md",
        "LICENSE"
    ],
    "engines": {
        "node": ">=16.0.0"
    }
}
